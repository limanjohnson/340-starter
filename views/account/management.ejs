
<main>
  <h1>Account Management</h1>
  <% if (messages.notice) { %>
    <p class="flash-message"><%= messages.notice %></p>
  <% } %>
  <% if (errors && errors.length > 0) { %>
    <ul class="errors">
      <% errors.forEach(error => { %>
        <li><%= error.msg %></li>
      <% }) %>
    </ul>
  <% } %>
  <p>You're logged in.</p>
</main>